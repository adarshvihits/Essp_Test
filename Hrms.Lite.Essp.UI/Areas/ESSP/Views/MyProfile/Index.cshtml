@model Hrms.Lite.Essp.Shared.ESSP.MyProfile.MyProfile
@{
    ViewData["Title"] = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
 <!--begin::Header-->
    <div id="kt_header" class="header header-fixed">
        <!--begin::Container-->
        <div class="container-fluid d-flex align-items-stretch justify-content-between">
            <!--begin::Header Menu Wrapper-->

            <div class="header-menu-wrapper header-menu-wrapper-left" id="kt_header_menu_wrapper">
                <!--begin::Header Menu-->
                <div class="d-flex align-items-baseline flex-wrap mr-5">
                    <h3 class="main-title"> My Profile</h3>
                    <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
                        <li class="breadcrumb-item ">
                            <a href="@Url.Action("Index", "Home", new { Area = "" })"><i class="icon-nm fas fa-home"></i></a>
                        </li>                      
                        <li class="breadcrumb-item">
                            My Profile
                        </li>

                    </ul>
                </div>
            </div>
         
        </div>
        <!--end::Container-->
    </div>
    <!--end::Header-->
    <div class="profile-header padding-big">
        <div class="row">

            <div class="col-md-9 offset-md-2 pl-15">
                <div class="row">

                    <div class="flex-grow-1" style="position: relative; top:-14px;">
                        <!--begin::Title-->
                        <div class="d-flex justify-content-between flex-wrap">
                            <div class="d-flex mr-3 header-profile-name">
                                <a href="#" class="header-profile-name" style="font-size: 24px;">@Model.EmployeeHeader.EmployeeName</a>


                            </div>

                            <div class="justify-content-between content-right">

                                <a href="@Url.Action("Create", "MyProfile", new { Area = "ESSP" })" class="my_profile_btn_edite"></a>
                            </div>

                        </div>

 <!--end::Title-->
 <!--begin::Content-->
                        <div class="d-flex flex-wrap justify-content-between mt-1">
                            <div class="d-flex flex-column flex-grow-1 pr-8">
                                <div class="d-flex flex-wrap mb-1">
                                    <a href="#" class="header-profile-email mr-lg-8 mr-5 mb-lg-0 mb-2">
                                        <i class="fas fa-briefcase icon-nm mr-2"></i>@Model.EmployeeHeader.Designation.Name
                                    </a>



                                </div>



                            </div>


                        </div>

 <!--end::Content-->
                    </div>

                </div>







            </div>
        </div>



        <div class="profile-header-overflow"></div>
    </div>
    <!--begin::Content-->

    <div class="content d-flex flex-column flex-column-fluid mt-35" id="kt_content">
        <!--begin::Entry-->
        <div class="d-flex flex-column-fluid backgrount-white">
            <!--begin::Container-->
            <div class="container full-width">
                <!--begin::Dashboard-->
                <!--begin::Row-->

                <div class="row height-100per position-relative z-index-1">
                    <div class="col-md-3">

                        <div class="card card-custom mb-5 height-100per backgrount-light-gray" style="margin-top: -10px;">

                            <div class="card-body p-5">

                                <div class="employee-details-left">
                                    <div class="employee-detail-image">
                                        @*<img src="images/sample-img.jpg" alt="">*@
                                        <img src="@Model.EmployeeHeader.EmployeeAbsoluteUri" alt="">
                                    </div>
                                    <div class="employee-outer-detils pt-14">
                                        <span><i class="far fas fa-mobile-alt icon-nm"></i> @Model.Address.PresentAddress.MobileNo</span>
                                        <span><i class="fas fa-envelope icon-nm"></i> @Model.Address.PresentAddress.MailID</span>
                                    </div>
                                    <div class="employee-outer-detils">
                                        <h4>Date of Join</h4>
                                        <span>@Model.DOJ</span>
                                        <span>@Model.YOS</span>
                                    </div>
                                    <div class="employee-outer-detils">
                                        <span><i class="fas fa-hashtag icon-nm"></i>@Model.EmployeeHeader.EmployeeId</span>
                                        <span><i class="fas fa-network-wired icon-nm"> </i>@Model.EmployeeHeader.Category.Name</span>
                                        <span><i class="fab fa-black-tie icon-nm"> </i>@Model.EmployeeHeader.Designation.Name</span>
                                        <span><i class="fas fa-id-card-alt icon-nm"> </i>@Model.EmployeeHeader.Department.Name</span>
                                        <span><i class="fas fa-map-marker-alt icon-nm"> </i>@Model.EmployeeHeader.Location.Name</span>
                                    </div>
                                    <div class="employee-outer-detils border-none">
                                        <h4>Reporting Manager</h4>
                                        <div class="employee-manager-img"><img src="@Model.ReportingManagerAbsoluteUri" alt=""></div>
                                        <div class="employee-manager-detils-content">

                                            <span><b>@Model.ReportingManager</b></span>
                                            <span>@Model.ReportingManagerDesignation</span>
                                        </div>

                                    </div>
                                </div>


                            </div>


                        </div>




                    </div>
                    <div class="col-md-9">
                        <div class="personal-details-left">

                            <div class="tab-outer accodiaton-only">
                                <ul class="ulList">
                                    <!-- Personal Tab -->
                                    <li class="tabOne clickOnTab active">Personal</li>
                                    <div id="tabOne" class="tab-content active tab-content-scroller">
                                        <div class="row">
                                            <div class="col-md-12">

                                                <h5><i class="fas fa-briefcase icon-md mr-2"></i> Address</h5>
                                                <div class="two-col">
                                                    <div class="col-100per">
                                                        <h4>Permanent Address</h4>

                                                        <div class="row-outer">
                                                            <label class="view-title">House Number, Building</label>
                                                            <div class="view-details">@Model.Address.PermanentAddress.House</div>
                                                        </div>
                                                        <div class="row-outer">
                                                            <label class="view-title">Street, Area</label>
                                                            <div class="view-details">@Model.Address.PermanentAddress.Street</div>
                                                        </div>
                                                        <div class="row-outer">
                                                            <label class="view-title">Locality, City</label>
                                                            <div class="view-details">@Model.Address.PermanentAddress.City</div>
                                                        </div>
                                                        <div class="row-outer">
                                                            <label class="view-title">Pin Code</label>
                                                            <div class="view-details">@Model.Address.PermanentAddress.PinCode</div>
                                                        </div>
                                                        <div class="row-outer">
                                                            <label class="view-title">Country</label>
                                                            <div class="view-details">@Model.Address.PermanentAddress.Country.Name</div>
                                                        </div>
                                                        <div class="row-outer">
                                                            <label class="view-title">State</label>
                                                            <div class="view-details">@Model.Address.PermanentAddress.State.Name</div>
                                                        </div>
                                                        <div class="row-outer">
                                                            <label class="view-title">District</label>
                                                            <div class="view-details">@Model.Address.PermanentAddress.District.Name</div>
                                                        </div>

                                                    </div>
                                                    <div class="col-100per">
                                                        <h4>Present Address</h4>

                                                        <div class="row-outer">
                                                            <label class="view-title">House Number, Building</label>
                                                            <div class="view-details">@Model.Address.PresentAddress.House</div>
                                                        </div>
                                                        <div class="row-outer">
                                                            <label class="view-title">Street, Area</label>
                                                            <div class="view-details">@Model.Address.PresentAddress.Street</div>
                                                        </div>
                                                        <div class="row-outer">
                                                            <label class="view-title">Locality, City</label>
                                                            <div class="view-details">@Model.Address.PresentAddress.City</div>
                                                        </div>
                                                        <div class="row-outer">
                                                            <label class="view-title">Pin Code</label>
                                                            <div class="view-details">@Model.Address.PresentAddress.PinCode</div>
                                                        </div>
                                                        <div class="row-outer">
                                                            <label class="view-title">Country</label>
                                                            <div class="view-details">@Model.Address.PresentAddress.Country.Name</div>
                                                        </div>
                                                        <div class="row-outer">
                                                            <label class="view-title">State</label>
                                                            <div class="view-details">@Model.Address.PresentAddress.State.Name</div>
                                                        </div>
                                                        <div class="row-outer">
                                                            <label class="view-title">District</label>
                                                            <div class="view-details">@Model.Address.PresentAddress.District.Name</div>
                                                        </div>

                                                    </div>
                                                </div>
                                                <h5><i class="fas fa-briefcase icon-md mr-2"></i>Contact Details</h5>

                                                <div class="three-col">

                                                    <div class="row-outer">
                                                        <label class="view-title">Personal Mobile</label>
                                                        <div class="view-details">@Model.ContactDetails.PersonalMobile</div>
                                                    </div>



                                                    <div class="row-outer">
                                                        <label class="view-title">Personal Email</label>
                                                        <div class="view-details">@Model.ContactDetails.PersonalEmail</div>
                                                    </div>



                                                    <div class="row-outer">
                                                        <label class="view-title">Emergency Contact Person</label>
                                                        <div class="view-details">@Model.ContactDetails.EmergencyContactPerson</div>
                                                    </div>
                                                    <div class="row-outer">
                                                        <label class="view-title">Emergency Contact Number</label>
                                                        <div class="view-details">@Model.ContactDetails.EmergencyContactNumber</div>
                                                    </div>
                                                    <div class="row-outer">
                                                        <label class="view-title">Relation</label>
                                                        <div class="view-details">@Model.ContactDetails.Relation.Name</div>
                                                    </div>


                                                </div>
                                                <h5><i class="fas fa-briefcase icon-md mr-2"></i>Personal Info</h5>
                                                <div class="three-col">
                                                    <div class="row-outer">
                                                        <label class="view-title">Gender</label>
                                                        <div class="view-details">@Model.EmployeeBasicInfo.Gender.Name</div>
                                                    </div>
                                                    <div class="row-outer">
                                                        <label class="view-title">DOB</label>
                                                        <div class="view-details">@Html.Encode(String.Format("{0:dd-MMM-yyyy}", Model.EmployeeBasicInfo.DOB))</div>
                                                    </div>
                                                    <div class="row-outer">
                                                        <label class="view-title">Blood Group</label>
                                                        <div class="view-details">@Model.PersonalInfo.BloodGroup.Name</div>
                                                    </div>

                                                    <div class="row-outer">
                                                        <label class="view-title">Marital Status</label>
                                                        <div class="view-details">@Model.PersonalInfo.MaritalStatus.Name</div>
                                                    </div>

                                                    <div class="row-outer">
                                                        <label class="view-title">DOW</label>
                                                        <div class="view-details">@Html.Encode(String.Format("{0:dd-MMM-yyyy}", Model.PersonalInfo.wedding))</div>
                                                    </div>
                                                    <div class="row-outer">
                                                        <label class="view-title">Religious View</label>
                                                        <div class="view-details">@Model.PersonalInfo.ReligiousView.Name</div>
                                                    </div>



                                                </div>
                                                <h5><i class="fas fa-briefcase icon-md mr-2"></i>Family Details</h5>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="ihits-table table-border hover-color mobile-responsive-table personal-details-family-section mb-5">
                                                            <table>
                                                                <thead>
                                                                    <tr class="table-light">
                                                                        <th scope="col">Name</th>
                                                                        <th scope="col">Relation</th>
                                                                        <th scope="col">Date of Birth</th>
                                                                        <th scope="col">Occupation</th>
                                                                        <th scope="col">Status</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    @if (Model.FamilyDetails != null && Model.FamilyDetails.Count > 0)
                                                                                {

                                                                                    for (int i = 0; i < Model.FamilyDetails.Count; i++)
                                                                                    {
                                                                    <tr>
                                                                        <td data-label="Name">
                                                                            <label>@Model.FamilyDetails[i].Name</label>

                                                                        </td>
                                                                        <td data-label="Relation">
                                                                            <label>@Model.FamilyDetails[i].Relation</label>

                                                                        </td>
                                                                        <td data-label="Date of Birth">
                                                                            <label>@Html.Encode(String.Format("{0:dd-MMM-yyyy}", Model.FamilyDetails[i].DateOfBirth))</label>



                                                                        </td>
                                                                        <td data-label="Occupation">
                                                                            <label>@Model.FamilyDetails[i].Occupation</label>

                                                                        </td>
                                                                        <td data-label="Status">
                                                                            <label>@Model.FamilyDetails[i].Status</label>

                                                                        </td>

                                                                    </tr>


                                                                                    }
                                                                                }
                                                                                else
                                                                                {
                                                                    <tr><td colspan="5"> No Data Found</td></tr>
                                                                                }






                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    </div>
                                                </div>
                                                <h5><i class="fas fa-briefcase icon-md mr-2"></i>Hobbies</h5>
                                                <div class="three-col">
                                                    <div class="row-outer">
                                                        <label class="view-title">Hobbies</label>
                                                        <div class="view-details">
                                                            @Model.Hobbies
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>
                                    <!-- Personal Tab -->
                                    <!-- Knowledge Base Tab -->
                                    <li class="tabContactDetails clickOnTab">Knowledge Base                                                </li>
                                    <div id="tabContactDetails" class="tab-content tab-content-scroller">
                                        <h5><i class="fas fa-briefcase icon-md mr-2"></i>Education</h5>
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="ihits-table table-border mobile-responsive-table personal-details-education-section mb-5">
                                                    <table>
                                                        <thead>
                                                            <tr class="table-light">
                                                                <th scope="col">Qualification Type</th>
                                                                <th scope="col">Qualification</th>
                                                                <th scope="col">Specialization of Branch</th>
                                                                <th scope="col">Year of Completion</th>

                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @if (Model.Education != null && Model.Education.Count > 0)
                                                                        {

                                                                            for (int i = 0; i < Model.Education.Count; i++)
                                                                            {
                                                            <tr>
                                                                <td data-label="Qualification Type">
                                                                    <label>@Model.Education[i].QualificationTypeName</label>

                                                                </td>
                                                                <td data-label="Qualofication">
                                                                    <label>@Model.Education[i].QualificationName</label>

                                                                </td>
                                                                <td data-label="Field of Study">
                                                                    <label>@Model.Education[i].SpecialisationName</label>

                                                                </td>
                                                                <td data-label="Year of Completion">
                                                                    <label>@Model.Education[i].YearOfCompletionName</label>

                                                                </td>



                                                            </tr>


                                                                            }
                                                                        }
                                                                        else
                                                                        {
                                                            <tr><td colspan="5"> No Data Found</td></tr>
                                                                        }





                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>


                                        <h5><i class="fas fa-briefcase icon-md mr-2"></i>Skills</h5>
                                        <div class="three-col">
                                            <div class="row-outer">
                                                <label class="view-title">Skill</label>
                                                <div class="view-details">@Model.Skills.Skill.Name</div>
                                            </div>
                                            <div class="row-outer">
                                                <label class="view-title">Skill Proficiency</label>
                                                <div class="view-details">@Model.Skills.SkillProficiency.Name</div>
                                            </div>
                                            <div class="row-outer">
                                                <label class="view-title">Skill Type</label>
                                                <div class="view-details">@Model.Skills.SkillType.Name</div>
                                            </div>

                                        </div>


                                        <h5><i class="fas fa-briefcase icon-md mr-2"></i>Previous Employment</h5>
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="ihits-table table-border mobile-responsive-table personal-details-previous-employment ">
                                                    <table>
                                                        <thead>
                                                            <tr class="table-light">
                                                                <th scope="col">Company Name</th>
                                                                <th scope="col">Location</th>
                                                                <th scope="col">Designation</th>
                                                                <th scope="col">From</th>
                                                                <th scope="col">To</th>
                                                                <th scope="col">Experience</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @if (Model.PreviousEmployment != null && Model.PreviousEmployment.Count > 0)
                                                                        {

                                                                            for (int i = 0; i < Model.PreviousEmployment.Count; i++)
                                                                            {
                                                            <tr>
                                                                <td data-label="Company Name & Address">
                                                                    <label>@Model.PreviousEmployment[i].CompanyName</label>
                                                                </td>
                                                                <td data-label="Location">
                                                                    <label>@Model.PreviousEmployment[i].Location</label>

                                                                </td>
                                                                <td data-label="Designation">
                                                                    <label>@Model.PreviousEmployment[i].Designation</label>

                                                                </td>
                                                                <td data-label="From">
                                                                    <label>@Html.Encode(String.Format("{0:dd-MMM-yyyy}", Model.PreviousEmployment[i].Fromdate))</label>

                                                                </td>
                                                                <td data-label="To">
                                                                    <label>@Html.Encode(String.Format("{0:dd-MMM-yyyy}", Model.PreviousEmployment[i].Todate))</label>

                                                                </td>
                                                                <td data-label="Experience">
                                                                    <label>@Model.PreviousEmployment[i].Experience</label>

                                                                </td>


                                                            </tr>

                                                                            }
                                                                        }
                                                                        else
                                                                        {
                                                            <tr><td colspan="5"> No Data Found</td></tr>
                                                                        }





                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>

                                        <h5><i class="fas fa-briefcase icon-md mr-2"></i>Languages</h5>
                                        <div class="three-col">
                                            <div class="row-outer">
                                                <label class="view-title">Languages</label>
                                                <div class="view-details">
                                                    @Model.Languages
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- Knowledge Base Tab -->
                                    <!-- Compensation Tab -->
                                    <li class="tabTwo clickOnTab">Compensation                                                </li>
                                    <div id="tabTwo" class="tab-content tab-content-scroller">
                                        <h5><i class="fas fa-briefcase icon-md mr-2"></i>Compensation & Benefits</h5>
                                        <div class="three-col ">
                                            <div class="row-outer">
                                                <label class="view-title">Effect from</label>
                                                <div class="view-details">@Html.Encode(String.Format("{0:dd-MMM-yyyy}", Model.Compensation.EffectFrom))</div>
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="ihits-table table-border vertical-scroll-150 table-head-small hover-color odd-even compensationBenefits">
                                                    <table>
                                                        <thead>
                                                            <tr class="table-light">
                                                                <th scope="col">Allowance</th>
                                                                <th scope="col">Type</th>
                                                                <th scope="col">Amount</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @if (Model.Compensation.FixedAllowance != null && Model.Compensation.FixedAllowance.Count > 0)
                                                                        {

                                                                            for (int i = 0; i < Model.Compensation.FixedAllowance.Count; i++)
                                                                            {
                                                            <tr>
                                                                <td data-label="Allowance">@Model.Compensation.FixedAllowance[i].Name</td>
                                                                <td data-label="Type">@Model.Compensation.FixedAllowance[i].AllowanceType</td>
                                                                <td data-label="Amount">@Model.Compensation.FixedAllowance[i].Amount</td>

                                                            </tr>



                                                                            }
                                                                        }
                                                                        else
                                                                        {
                                                            <tr><td colspan="5"> No Data Found</td></tr>
                                                                        }

                                                        </tbody>

                                                    </table>
                                                </div>

                                            </div>
                                            <div class="col-md-6">
                                                <div class="ihits-table table-border table-head-small vertical-scroll-150 hover-color odd-even compensationBenefits">
                                                    <table>
                                                        <thead>
                                                            <tr class="table-light">
                                                                <th scope="col">Deduction</th>
                                                                <th scope="col">Amount</th>

                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @if (Model.Compensation.FixedDeduction != null && Model.Compensation.FixedDeduction.Count > 0)
                                                                        {

                                                                            for (int i = 0; i < Model.Compensation.FixedDeduction.Count; i++)
                                                                            {

                                                            <tr>
                                                                <td data-label="Deduction">@Model.Compensation.FixedDeduction[i].Name</td>
                                                                <td data-label="Amount">@Model.Compensation.FixedDeduction[i].Amount</td>

                                                            </tr>
                                                                            }
                                                                        }
                                                                        else
                                                                        {
                                                            <tr><td colspan="5"> No Data Found</td></tr>
                                                                        }

                                                        </tbody>

                                                    </table>
                                                </div>

                                            </div>
                                        </div>

                                        <div class="four-col border-none">
                                            <div class="row-outer">
                                                <label class="view-title">Gross Salary</label>
                                                <div class="view-details">@Model.Compensation.GrossSalary</div>
                                            </div>
                                            <div class="row-outer">
                                                <label class="view-title">CTC</label>
                                                <div class="view-details">@Model.Compensation.CTC</div>
                                            </div>

                                            <div class="row-outer">
                                                <label class="view-title">PF</label>
                                                <div class="view-details">
                                                    @if (Model.Compensation.PF == true)
                                                                  {
                                                    <button type="button" class="btn btn-icon btn-tick border-radius"><i class="icon-xl fas fa-check"></i></button>
                                                                  }
                                                                  else
                                                                  {
                                                    <button type="button" class="btn btn-icon btn-close border-radius"><i class="icon-xl fas fa-times"></i></button>
                                                                  }
                                                </div>
                                            </div>

                                            <div class="row-outer">
                                                <label class="view-title">ESI</label>
                                                <div class="view-details">
                                                    @if (Model.Compensation.ESI == true)
                                                                  {
                                                    <button type="button" class="btn btn-icon btn-tick border-radius"><i class="icon-xl fas fa-check"></i></button>
                                                                  }
                                                                  else
                                                                  {
                                                    <button type="button" class="btn btn-icon btn-close border-radius"><i class="icon-xl fas fa-times"></i></button>
                                                                  }
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <!-- Compensation Tab -->
                                    <!-- Career Tab -->
                                    <li class="tabThree clickOnTab">Career</li>
                                    <div id="tabThree" class="tab-content tab-content-scroller">
                                        <h5><i class="fas fa-briefcase icon-md mr-2"></i>Career Progression</h5>
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="ihits-table table-border table-head-small mobile-responsive-table ">
                                                    <table>
                                                        <thead>
                                                            <tr class="table-light">
                                                                <th scope="col">Type</th>
                                                                <th scope="col">Effective Date</th>
                                                                <th scope="col">Department</th>
                                                                <th scope="col">Designation</th>
                                                                <th scope="col">Location</th>
                                                                <th scope="col">Category</th>
                                                                <th scope="col" style="text-align: right;">
                                                                    Gross Pay
                                                                </th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @if (Model.GrowthHistory != null && Model.GrowthHistory.Count > 0)
                                                                        {

                                                                            for (int i = 0; i < Model.GrowthHistory.Count; i++)
                                                                            {
                                                            <tr>
                                                                <td data-label="Type ">@Model.GrowthHistory[i].TransitionType</td>
                                                                <td data-label="Effective Date">
                                                                    @Html.Encode(String.Format("{0:dd-MMM-yyyy}", Model.GrowthHistory[i].EffectiveDate))

                                                                </td>
